{
  "name": "fetch-helpers",
  "version": "0.0.0",
  "description": "Response handlers and helpers to parse JSON and deal with HTTP errors when using the browser fetch API.",
  "keywords": [
    "fetch",
    "checkStatus",
    "parseJSON",
    "parseJSONError",
    "batch"
  ],
  "author": "Chad Lee",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "compile": "rimraf es lib && cross-env BABEL_ENV=es babel src -d es && cross-env BABEL_ENV=cjs babel src -d lib",
    "lint": "eslint src test",
    "test": "cross-env BABEL_ENV=cjs mocha --recursive --require babel-core/register --require babel-polyfill"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chadly/fetch-helpers.git"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "lodash": "4.x",
    "shallowequal": "1.x"
  },
  "peerDependencies": {
    "react": ">= 15.x < 17"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^4.9.0",
    "eslint-config-civicsource": "^8.0.0",
    "jsdom": "^11.3.0",
    "json-update": "^3.0.0",
    "mocha": "^4.0.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.2"
  }
}
