{
  "name": "fetch-helpers",
  "version": "1.0.3",
  "description": "Response handlers and helpers to parse JSON and deal with HTTP errors when using the browser fetch API.",
  "keywords": [
    "fetch",
    "checkStatus",
    "parseJSON",
    "parseJSONError"
  ],
  "author": "Chad Lee",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel -d lib src",
    "prepublish": "npm run compile"
  },
  "babel": {
    "presets": ["es2015", "stage-2"],
    "plugins": ["lodash"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chadly/fetch-helpers.git"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-plugin-lodash": "3.x",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-2": "^6.1.18"
  },
  "dependencies": {
    "lodash": "4.x"
  }
}
