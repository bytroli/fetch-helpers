{
  "name": "fetch-helpers",
  "version": "3.0.0",
  "description": "Response handlers and helpers to parse JSON and deal with HTTP errors when using the browser fetch API.",
  "keywords": [
    "fetch",
    "checkStatus",
    "parseJSON",
    "parseJSONError",
    "batch"
  ],
  "author": "Chad Lee",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "compile-cjs": "rimraf lib && cross-env BABEL_ENV=cjs babel src -d lib",
    "compile-es": "rimraf es && cross-env BABEL_ENV=es babel src -d es",
    "compile": "npm run compile-cjs && npm run compile-es",
    "lint": "eslint src",
    "test": "cross-env BABEL_ENV=cjs mocha --compilers js:babel-core/register --recursive --require babel-polyfill",
    "prepublish": "npm run lint && npm test && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chadly/fetch-helpers.git"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "lodash": "4.x",
    "shallowequal": "1.x"
  },
  "peerDependencies": {
    "react": ">= 15.x"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "4.x",
    "cross-env": "5.x",
    "enzyme": "^2.9.1",
    "eslint": "4.x",
    "eslint-config-civicsource": "6.x",
    "jsdom": "^11.1.0",
    "mocha": "3.x",
    "react": "15.x",
    "react-dom": "15.x",
    "react-test-renderer": "^15.6.1",
    "rimraf": "2.x"
  }
}
