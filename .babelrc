{
	"plugins": [
		"lodash",
		"@babel/proposal-export-default-from",
		"@babel/proposal-class-properties",
		["@babel/proposal-object-rest-spread", { "useBuiltIns": true }]
	],
	"env": {
		"es": {
			"presets": ["@babel/react"]
		},
		"node": {
			"presets": [["@babel/env", {
				"targets": {
				  "node": "current"
				}
			}], "@babel/react"]
		}
	}
}