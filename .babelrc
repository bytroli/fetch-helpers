{
	"plugins": [
		"lodash",
		"transform-export-extensions",
		["transform-object-rest-spread", { "useBuiltIns": true }]
	],
	"env": {
		"es": {
			"presets": [["env", {
				"targets": {
				  "node": "current"
				},
				"modules": false,
				"useBuiltIns": "usage"
			}], "react"]
		},
		"cjs": {
			"presets": [["env", {
				"targets": {
				  "browsers": "last 2 versions"
				},
				"useBuiltIns": "usage"
			}], "react"]
		}
	}
}